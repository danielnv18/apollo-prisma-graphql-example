{
  "name": "pm-backend",
  "version": "0.1.0",
  "description": "Graphql Api for Project manangment",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielnv18/pm-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielnv18/pm-backend/issues"
  },
  "homepage": "https://github.com/danielnv18/pm-backend#readme",
  "scripts": {
    "deploy": "prisma deploy --env-file .env",
    "start": "npm run serve",
    "serve": "node -r dotenv/config dist/index.js",
    "watch-node": "nodemon --exec \"node -r dotenv/config\" dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "graphql": "^14.0.2",
    "graphql-cli": "^3.0.3",
    "graphql-yoga": "^1.16.9",
    "prisma": "^1.23.2",
    "prisma-binding": "^2.2.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.3",
    "@types/node": "^10.12.18",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "typescript": "^3.2.2"
  }
}
